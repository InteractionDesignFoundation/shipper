{
  "name": "shipper",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "format": "yarn run eslint --fix --ext .js,.vue src,tests",
    "lint": "vue-cli-service lint",
    "test": "yarn test:unit",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "compare-versions": "^3.5.1",
    "emoji-regex": "^8.0.0",
    "vue": "^2.6",
    "wretch": "^1.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1",
    "@vue/cli-plugin-eslint": "^3.12",
    "@vue/cli-plugin-unit-jest": "^4.2",
    "@vue/cli-service": "^4.1",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^25.1",
    "vue-template-compiler": "^2.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 4 Chrome versions",
    "Firefox ESR",
    "last 2 Safari versions",
    "last 1 Edge versions"
  ],
  "author": "Interaction Design Foundation",
  "description": "IDF Release helper: helps to create Releases and manage milestones on GitHub"
}
